{
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.saveFileBeforeRun": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 20,
    "editor.language.colorizedBracketPairs": [
        
    
    
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],

    ],

    "workbench.colorCustomizations": {

         // Bracket colors
         "editorBracketHighlight.foreground1": "#FFD700",
         "editorBracketHighlight.foreground2": "#e72be1",
         "editorBracketHighlight.foreground3": "#179effa2",
         "editorBracketHighlight.foreground4": "#43e91a8c",
         // Inactive guide colors
         "editorBracketPairGuide.background1": "#ac43fc53",
         "editorBracketPairGuide.background2": "#ac43fc53",
         "editorBracketPairGuide.background3": "#ac43fc53",
         "editorBracketPairGuide.background4": "#ac43fc53",
         // Active guide colors
         "editorBracketPairGuide.activeBackground1": "#fffb00",
         "editorBracketPairGuide.activeBackground2": "#fffb00",
         "editorBracketPairGuide.activeBackground3": "#ffff00",
         "editorBracketPairGuide.activeBackground4": "#fffb00",

    },
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.highlightActiveBracketPair": true,
    "prettier.bracketSameLine": true,
    "editor.inlayHints.padding": true,
    "editor.inlayHints.enabled": "on",
    "javascript.inlayHints.enumMemberValues.enabled": false,
    "javascript.inlayHints.parameterTypes.enabled": false,
    "javascript.inlayHints.variableTypes.enabled": false,
    "javascript.inlayHints.propertyDeclarationTypes.enabled": false,

    // "editor.language.brackets": [

    //     ["{", "}"],
    //     ["[", "]"],
    //     ["(", ")"],
    //     ["<", ">"],
    
    // ]
}